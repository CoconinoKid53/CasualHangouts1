DROP TABLE IF EXISTS public.posts, public.users;
-- Table structure for table `posts`
CREATE TABLE IF NOT EXISTS public.posts (
	post_id integer NOT NULL GENERATED BY DEFAULT AS IDENTITY
,	user_id integer NOT NULL
,	event_des character varying NOT NULL
,	address character varying NOT NULL
,	start_at TIME NOT NULL
,	end_at TIME NOT NULL
,	CONSTRAINT posts_pkey PRIMARY KEY (post_id)
);
-- Table structure for table `users`
CREATE TABLE IF NOT EXISTS public.users (
	user_id integer NOT NULL GENERATED BY DEFAULT AS IDENTITY
,	username character varying NOT NULL
,	device_token character varying NOT NULL
,	CONSTRAINT user_pkey PRIMARY KEY (user_id)
);
ALTER TABLE IF EXISTS public.posts
ADD CONSTRAINT fk_posts FOREIGN KEY (user_id) REFERENCES public.users (user_id) MATCH SIMPLE ON UPDATE CASCADE ON DELETE NO ACTION;

